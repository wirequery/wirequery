{
  "name": "manager",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "npm run generate:schema && TZ=UTC NODE_ENV=test jest --passWithNoTests",
    "test:dev": "npm run generate:schema && TZ=UTC NODE_ENV=test jest --watchAll",
    "start": "NODE_ENV=production PORT=$PORT next start",
    "build": "npm run generate:schema && rm -rf .next && next build",
    "build-no-schema": "rm -rf .next && next build",
    "dev": "npm run generate:schema && next dev",
    "export": "npm run build && next export",
    "prettier": "prettier --write **/*.{ts,tsx,js}",
    "lint:ts": "npm run prettier && eslint --fix 'src/**/*.{ts,tsx}' '__tests__/**/*.{ts,tsx}'",
    "lint:scss": "stylelint --fix 'src/**/*.scss'",
    "lint:types": "tsc -p . --noEmit",
    "generate:schema": "graphql-codegen --config codegen.yml"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.21",
    "@mantine/dates": "^6.0.21",
    "@mantine/ds": "^6.0.21",
    "@mantine/form": "^6.0.21",
    "@mantine/hooks": "^6.0.21",
    "@mantine/notifications": "^6.0.21",
    "@rrweb/types": "^2.0.0-alpha.11",
    "@tabler/icons-react": "^2.14.0",
    "apexcharts": "^3.43.0",
    "d3": "^7.8.5",
    "dayjs": "^1.11.9",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "graphql-sse": "^2.3.0",
    "next": "^13.2.4",
    "next-images": "^1.8.5",
    "next-query-params": "^4.2.1",
    "next-urql": "^5.0.0",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-json-tree": "^0.18.0",
    "rrweb": "^2.0.0-alpha.11",
    "rrweb-player": "^1.0.0-alpha.4",
    "urql": "^4.0.0",
    "use-query-params": "^2.2.1",
    "wonka": "^6.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "^3.0.3",
    "@graphql-codegen/typescript-document-nodes": "3.0.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/d3": "^7.4.2",
    "@types/react": "18.0.33",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "13.2.0",
    "postcss-preset-env": "8.3.0",
    "prettier": "2.8.7",
    "sass": "1.60.0",
    "stylelint": "15.4.0",
    "typescript": "5.0.3"
  }
}